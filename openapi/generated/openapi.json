{
  "openapi": "3.0.0",
  "components": {
    "schemas": {
      "CreateAppointment": {
        "type": "object",
        "required": [
          "insuredId",
          "countryISO",
          "schedule"
        ],
        "properties": {
          "insuredId": {
            "type": "string",
            "example": "00001",
            "pattern": "^[0-9]{5}$"
          },
          "countryISO": {
            "type": "string",
            "example": "PE"
          },
          "schedule": {
            "type": "object",
            "required": [
              "date",
              "centerId",
              "specialtyId",
              "medicId",
              "scheduleId"
            ],
            "properties": {
              "date": {
                "type": "string",
                "format": "date-time",
                "example": "2025-08-29T15:00:00Z"
              },
              "centerId": {
                "type": "string",
                "example": "REBG"
              },
              "specialtyId": {
                "type": "string",
                "example": "TRAS"
              },
              "medicId": {
                "type": "string",
                "example": "TRAS_REBG_PE_02"
              },
              "scheduleId": {
                "type": "integer",
                "example": 77511
              }
            }
          }
        }
      },
      "AppointmentCreated": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "example": "Su cita registró con éxito con el código: 01K1SEQJXKCG01C58D6M8MR88N"
          }
        }
      },
      "MedicalCenter": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "REBG"
          },
          "name": {
            "type": "string",
            "example": "Edgardo Rebagliati Martins"
          },
          "countryISO": {
            "type": "string",
            "example": "PE"
          },
          "type": {
            "type": "string",
            "example": "Público EsSalud"
          },
          "specialties": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "example": [
              "CARD",
              "ONCO"
            ]
          }
        }
      },
      "MedicalCenterList": {
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/MedicalCenter"
        }
      },
      "Doctor": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "ONCO_REBG_PE_02"
          },
          "name": {
            "type": "string",
            "example": "Lucía"
          },
          "surname": {
            "type": "string",
            "example": "Vargas"
          },
          "gender": {
            "type": "string",
            "enum": [
              "M",
              "F"
            ],
            "example": "F"
          },
          "specialty": {
            "type": "string",
            "example": "Oncología"
          },
          "specialtyId": {
            "type": "string",
            "example": "ONCO"
          },
          "centerId": {
            "type": "string",
            "example": "REBG"
          },
          "countryISO": {
            "type": "string",
            "example": "PE"
          }
        }
      },
      "DoctorList": {
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/Doctor"
        }
      },
      "Appointment": {
        "type": "object",
        "properties": {
          "appointmentId": {
            "type": "string",
            "example": "01K1S471KN35XPCXHANTZ078G3"
          },
          "insuredId": {
            "type": "string",
            "example": "00001"
          },
          "doctorId": {
            "type": "string",
            "example": "OBST_LOAY_PE_01"
          },
          "medicalCenterId": {
            "type": "string",
            "example": "LOAY"
          },
          "specialtyId": {
            "type": "string",
            "example": "OBST"
          },
          "date": {
            "type": "string",
            "format": "date-time",
            "example": "2025-08-15T08:30:00Z"
          },
          "state": {
            "type": "string",
            "example": "pending"
          },
          "countryISO": {
            "type": "string",
            "example": "PE"
          }
        }
      },
      "AppointmentList": {
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/Appointment"
        }
      },
      "ErrorResponse": {
        "type": "object",
        "properties": {
          "error": {
            "type": "boolean",
            "example": true
          },
          "message": {
            "type": "string",
            "example": "Mensaje de error según el endpoint"
          }
        }
      },
      "NotFound": {
        "type": "object",
        "properties": {
          "error": {
            "type": "boolean",
            "example": true
          },
          "message": {
            "type": "string",
            "example": "No se encontró el recurso"
          }
        }
      },
      "UnexpectedResponse": {
        "type": "object",
        "properties": {
          "error": {
            "type": "boolean",
            "example": true
          },
          "message": {
            "type": "string",
            "example": "Error desconocido"
          }
        }
      },
      "insuredId": {
        "type": "string",
        "pattern": "^\\d{5}$",
        "example": "00001"
      },
      "centerId": {
        "type": "string",
        "example": "REBG"
      },
      "specialtyId": {
        "type": "string",
        "example": "ONCO"
      }
    }
  },
  "info": {
    "title": "Medical Appointment API",
    "description": "API para gestión de citas médicas",
    "version": "1"
  },
  "paths": {
    "/catalog/medical-centers/{insuredId}": {
      "get": {
        "summary": "Obtener centros médicos por código de asegurado",
        "description": "Devuelve la lista de centros médicos disponibles para un asegurado específico.",
        "operationId": "getCentersByInsuredId",
        "parameters": [
          {
            "name": "insuredId",
            "in": "path",
            "description": "Código del asegurado (5 dígitos, con ceros a la izquierda)",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/insuredId"
            }
          }
        ],
        "tags": [
          "Catalog Lambda"
        ],
        "responses": {
          "200": {
            "description": "Respuesta exitosa con la lista de centros médicos",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MedicalCenterList"
                }
              }
            },
            "headers": {}
          },
          "400": {
            "description": "Request inválido o error de servidor",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            },
            "headers": {}
          },
          "404": {
            "description": "No se encontraron centros médicos disponibles",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NotFound"
                }
              }
            },
            "headers": {}
          }
        }
      }
    },
    "/catalog/doctors": {
      "get": {
        "summary": "Lista doctores de un centro y especialidad",
        "description": "Devuelve la lista de doctores por centro médico y especialidad.",
        "operationId": "getDoctorsByCenterAndSpecialty",
        "parameters": [
          {
            "name": "centerId",
            "in": "query",
            "description": "ID del centro médico",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/centerId"
            }
          },
          {
            "name": "specialtyId",
            "in": "query",
            "description": "ID de la especialidad médica",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/specialtyId"
            }
          }
        ],
        "tags": [
          "Catalog Lambda"
        ],
        "responses": {
          "200": {
            "description": "Lista de doctores",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DoctorList"
                }
              }
            },
            "headers": {}
          },
          "400": {
            "description": "Error en los parámetros de búsqueda",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            },
            "headers": {}
          },
          "500": {
            "description": "Error interno",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UnexpectedResponse"
                }
              }
            },
            "headers": {}
          }
        }
      }
    },
    "/appointment": {
      "post": {
        "summary": "Crea una cita médica",
        "description": "Crea una cita para un asegurado. Estado inicial pending. Procesamiento asíncrono.",
        "operationId": "createAppointment",
        "parameters": [],
        "tags": [
          "Appointment Lambda"
        ],
        "requestBody": {
          "description": "Datos de la cita médica",
          "required": false,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateAppointment"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Cita creada correctamente",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AppointmentCreated"
                }
              }
            },
            "headers": {}
          },
          "400": {
            "description": "Error de validación",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            },
            "headers": {}
          },
          "500": {
            "description": "Error inesperado",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UnexpectedResponse"
                }
              }
            },
            "headers": {}
          }
        }
      }
    },
    "/appointment/insured/{insuredId}": {
      "get": {
        "summary": "Obtiene todas las citas por código de asegurado",
        "description": "Devuelve la lista de citas médicas asociadas a un asegurado.",
        "operationId": "getAppointmentsByInsuredId",
        "parameters": [
          {
            "name": "insuredId",
            "in": "path",
            "description": "Código de asegurado (5 dígitos, con ceros a la izquierda)",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/insuredId"
            }
          }
        ],
        "tags": [
          "Appointment Lambda"
        ],
        "responses": {
          "200": {
            "description": "Lista de citas del asegurado",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AppointmentList"
                }
              }
            },
            "headers": {}
          },
          "400": {
            "description": "Error de validación (ej. parámetro no enviado)",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            },
            "headers": {}
          },
          "404": {
            "description": "No se encontraron citas",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NotFound"
                }
              }
            },
            "headers": {}
          }
        }
      }
    }
  },
  "servers": [
    {
      "url": "https://9knrfdquid.execute-api.us-east-1.amazonaws.com"
    }
  ]
}